<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="duplicate_term_canada" model="res.partner.duplicate.term">
            <field name="type">string</field>
            <field name="expression">canada</field>
        </record>

        <record id="duplicate_term_inc" model="res.partner.duplicate.term">
            <field name="type">string</field>
            <field name="expression">inc.</field>
        </record>

        <record id="duplicate_term_regex_number" model="res.partner.duplicate.term">
            <field name="type">regex</field>
            <field name="expression">(^|\s+)\d+($|\s+)</field>
        </record>

    </data>
</odoo>
